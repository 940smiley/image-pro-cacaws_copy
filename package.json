{
  "name": "image-pro",
  "private": true,
  "version": "0.0.0",
  "description": "Image Pro - Advanced image processing and analysis application",
  "author": "940smiley",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "setup": "bash scripts/setup-dev-environment.sh",
    "setup:win": "scripts\\setup-dev-environment.bat",
    "build:all": "bash scripts/build-and-release.sh",
    "build:all:win": "scripts\\build-and-release.bat",
    "test": "npm run typecheck && npm run lint",
    "release": "bash scripts/build-and-release.sh",
    "electron:dev": "tsc electron/main.ts --outDir dist_electron && tsc electron/preload.ts --outDir dist_electron && electron dist_electron/main.js",
    "electron:build": "tsc electron/main.ts --outDir dist_electron && tsc electron/preload.ts --outDir dist_electron",
    "build:electron": "npm run build && npm run electron:build && electron-builder",
    "dist": "npm run build && npm run electron:build && electron-builder --publish=never"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "exiftool-vendored": "^34.2.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/electron": "^1.6.10",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "electron": "^33.0.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2",
    "electron": "^32.2.6",
    "electron-builder": "^26.0.12"
  },
  "main": "dist_electron/main.js",
  "build": {
    "appId": "com.940smiley.imagepro",
    "productName": "Image Pro",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "extraResources": [
      "public/**/*"
    ]
  }
  }
}
